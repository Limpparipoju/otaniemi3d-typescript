{
    "version": "1.5.1",
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "outDir": "build",
        "rootDir": "src"
    },
    "filesGlob": [
        "src/**/*.ts",
        "type_definitions/*.ts"
    ],
    "files": [
        "src/app.ts",
        "src/elements/app-layout/app-layout.ts",
        "src/elements/building-selector/building-selector.ts",
        "src/elements/floor-plan/floor-plan.ts",
        "src/elements/heat-map/heat-map.ts",
        "src/elements/map-info-window/map-info-window.ts",
        "src/elements/omi-message/omi-message.ts",
        "type_definitions/core.d.ts",
        "type_definitions/otaniemi3d.d.ts",
        "type_definitions/tsd.d.ts"
    ],
    "exclude": []
}
